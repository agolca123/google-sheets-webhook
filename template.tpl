___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require('logToConsole');
const sendPixel = require('sendPixel');
const encodeUriComponent = require('encodeUriComponent');

const webhookUrl = data.webhookUrl;
let webhookProperties = "";

// webhookProperties'ı URL parametrelerine dönüştürme
for (let i = 0; i < data.webhookProperties.length; i++) {
  webhookProperties += encodeUriComponent(data.webhookProperties[i].column1) + "=" + encodeUriComponent(data.webhookProperties[i].column2);
  if (i < data.webhookProperties.length - 1) {
    webhookProperties += '&';
  }
}

const fullUrl = webhookUrl + '?' + webhookProperties;

log('Sending data to:', fullUrl);

// HTTP GET request
sendPixel(fullUrl, data.gtmOnSuccess, data.gtmOnFailure);


